{
  "name": "beatui-monorepo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "test:e2e": "pnpm --filter @beatui/docs test:e2e",
    "test:e2e:all": "pnpm --filter @beatui/docs test:e2e:all",
    "test:e2e:smoke": "pnpm --filter @beatui/docs test:e2e:smoke",
    "test:e2e:a11y": "pnpm --filter @beatui/docs test:e2e:a11y",
    "test:e2e:visual": "pnpm --filter @beatui/docs test:e2e:visual",
    "test:e2e:ui": "pnpm --filter @beatui/docs test:e2e:ui",
    "test:e2e:update-snapshots": "pnpm --filter @beatui/docs test:e2e:update-snapshots",
    "lint": "turbo lint",
    "format": "turbo format",
    "typecheck": "turbo typecheck",
    "check": "turbo check",
    "clean": "turbo clean",
    "dev:docs": "pnpm --filter @tempots/beatui build && concurrently \"pnpm --filter @tempots/beatui build:watch\" \"pnpm --filter @beatui/docs dev\"",
    "showcase": "pnpm --filter @beatui/showcase dev"
  },
  "keywords": [],
  "author": "Franco Ponticelli",
  "license": "MIT",
  "description": "BeatUI Monorepo - Modern UI component library and documentation",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tempots/eslint-plugin": "0.8.1",
    "chokidar": "^5.0.0",
    "concurrently": "^9.2.1",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "turbo": "2.5.4",
    "typescript-eslint": "8.46.0"
  },
  "packageManager": "pnpm@10.21.0"
}

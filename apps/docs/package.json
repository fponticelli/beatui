{
  "name": "@beatui/docs",
  "version": "1.0.0",
  "type": "module",
  "description": "BeatUI Documentation Site",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npx tsx scripts/generate-static.ts && pnpm run validate-static",
    "validate-static": "npx tsx scripts/validate-static.ts",
    "serve": "serve .",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,js,json,css,html,md}\"",
    "typecheck": "tsc --noEmit",
    "gen:schema": "npx tsx scripts/gen-meta-schemas.ts"
  },
  "keywords": [
    "documentation",
    "beatui",
    "vite"
  ],
  "author": "Franco Ponticelli",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^14.2.0",
    "@tempots/beatui": "workspace:*",
    "@tempots/dom": "29.0.3",
    "@tempots/std": "0.22.1",
    "@tempots/ui": "7.0.3",
    "monaco-editor": "^0.52.2",
    "monaco-yaml": "^5.4.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "22.15.18",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "cheerio": "^1.0.0",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "fs-extra": "^11.2.0",
    "jsdom": "26.1.0",
    "prettier": "3.5.3",
    "serve": "^14.2.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.32.1",
    "vite": "^6.3.5"
  },
  "packageManager": "pnpm@10.11.0"
}

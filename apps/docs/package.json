{
  "name": "@beatui/docs",
  "version": "1.0.0",
  "type": "module",
  "description": "BeatUI Documentation Site",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npx tsx scripts/generate-static.ts",
    "serve": "serve .",
    "preview:vite": "vite preview",
    "clean": "rm -rf dist",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "documentation",
    "beatui",
    "vite"
  ],
  "author": "Franco Ponticelli",
  "license": "MIT",
  "dependencies": {
    "@tempots/beatui": "workspace:*",
    "@tempots/dom": "28.1.5",
    "@tempots/std": "0.19.0",
    "@tempots/ui": "4.3.8",
    "zod": "^3.25.46"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "22.15.18",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "cheerio": "^1.0.0",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "fs-extra": "^11.2.0",
    "prettier": "3.5.3",
    "serve": "^14.2.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.32.1",
    "vite": "^6.3.5"
  },
  "packageManager": "pnpm@10.11.0"
}
